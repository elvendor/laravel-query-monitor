            "providers": [
                "Supliu\\LaravelQueryMonitor\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^5.0"
    }
