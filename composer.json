{
    "name": "supliu/laravel-query-monitor",
    "type": "library",
    "description": "Laravel Query Monitor",
    "keywords": [
        "laravel", "eloquent", "query", "sql", "monitor"
    ],
    "authors": [
        {
            "name": "Jansen Felipe",
            "email": "jansen.felipe@gmail.com"
        }
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Supliu\\LaravelQueryMonitor\\": "src/"
        }
    },
    "require": {
        "laravel/framework": "^7.0|^8.0",
        "php": "^7.2.5",
        "react/socket": "^1.4"
    },
    "extra": {
